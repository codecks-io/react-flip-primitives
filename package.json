    "eslint": "5.7",
    "react-powerplug": "^1.0.0-rc.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "bezier-easing": "^2.1.0",
    "prop-types": "^15.6.2",
    "rematrix": "^0.3.0"
